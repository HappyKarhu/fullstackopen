{
  "name": "blogilista",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint frontend --ext .js,.jsx",
    "lint:fix": "eslint frontend --ext .js,.jsx --fix",
    "test": "vitest run",
    "cypress:open": "cypress open",
    "start:test": "cross-env NODE_ENV=test node backend/index.js",
